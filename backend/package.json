{
  "name": "rrent-backend",
  "version": "0.0.1",
  "description": "RRent backend API",
  "author": "RRent Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:auth-smoke": "jest --runTestsByPath test/auth-login.e2e-spec.ts",
    "test:create-chain": "jest --runTestsByPath test/create-chain.e2e-spec.ts",
    "test:be2-services": "jest --runTestsByPath test/be2-org-property-unit.spec.ts",
    "test:be2-services-2": "jest --runTestsByPath test/be2-tenant-lease-payment.spec.ts",
    "test:payment-flow": "jest --runTestsByPath test/payment-flow.e2e-spec.ts",
    "test:pagination": "NODE_ENV=test jest --runTestsByPath test/pagination.e2e-spec.ts",
    "test:cov": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.0.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.4.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4"
  }
}
